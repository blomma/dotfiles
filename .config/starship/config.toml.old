format = """
$username\
$hostname\
$shlvl\
$directory\
$vcsh\
$git_branch\
$git_commit\
$git_state\
$git_metrics\
$git_status\
$hg_branch\
$docker_context\
$package\
$red\
$memory_usage\
$env_var\
$custom\
$line_break\
$jobs\
$time\
$status\
$shell\
$character"""

[directory]
read_only = " "

[package]
symbol = " "

[shlvl]
symbol = " "

[git_branch]
format = '\[[$symbol$branch]($style)\]'
symbol = " "

[git_metrics]
disabled = false

[git_status]
format = '([\[$all_status$ahead_behind\]]($style))'

[memory_usage]
format = '\[$symbol[$ram( | $swap)]($style)\]'
symbol = " "

[time]
format = '\[[$time]($style)\]'

[username]
format = '\[[$user]($style)\]'

